<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="#ffffff"
              android:clipToPadding="true"
              android:fitsSystemWindows="true"
              android:orientation="vertical">

    <include
        android:id="@+id/in_titile_bar"
        layout="@layout/title_bar_include"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fitsSystemWindows="true" />


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.digoshop.app.module.storedetail.view.ListViewForScrollView
            android:id="@+id/lv_result_listview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@color/hui_color"
            android:dividerHeight="@dimen/base_dimen_1"
            android:visibility="visible"/>
    </ScrollView>
</LinearLayout>
